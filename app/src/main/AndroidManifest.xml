<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.practice.retrofit">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:allowBackup="true"
        android:icon="@mipmap/app_rest_api"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_rest_api_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Retrofit">
        <activity android:name=".dashboard.DashboardActivity"></activity>
        <activity android:name=".welcome.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>